<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import widgets.handDashboard.PawControl?>
<fx:root type="javafx.scene.layout.VBox"
          xmlns:fx="http://javafx.com/fxml">
    <PawControl fx:id="paw"></PawControl>
    <VBox>
        <HBox>
            <Label text="Grab: "></Label>
            <ProgressIndicator fx:id="grabIndicator"></ProgressIndicator>
        </HBox>
        <HBox>
            <Label text="Pinch: "></Label>
            <ProgressIndicator fx:id="pinchIndicator"></ProgressIndicator>
        </HBox>
        <HBox>
            <Label text="Confi: "></Label>
            <ProgressIndicator fx:id="confidenceIndicator"></ProgressIndicator>
        </HBox>
        <HBox>
            <Label text="Roll: "></Label>
            <ProgressIndicator fx:id="rollAngle"></ProgressIndicator>
        </HBox>
    </VBox>
</fx:root>